{
  "expo": {
    "name": "BrainSpy",
    "slug": "brainspy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/spy-brain.png",
    "scheme": "socialicebreakerai",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.santisrepo.brainspy",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.santisrepo.brainspy",
      "versionCode": 1
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/spy-brain.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "06851116-ebd0-4f34-a726-bed981c1862a"
      }
    }
  }
}
