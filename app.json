{
  "expo": {
    "name": "BrainSpy",
    "slug": "brainspy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/spy-brain.png",
    "scheme": "socialicebreakerai",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "jsEngine": "hermes",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.santisrepo.brainspy",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": false,
          "NSExceptionDomains": {
            "api.openai.com": {
              "NSExceptionAllowsInsecureHTTPLoads": false,
              "NSExceptionMinimumTLSVersion": "1.2"
            }
          }
        }
      },
      "icon": "./assets/images/spy-brain.png"
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "com.santisrepo.brainspy",
      "versionCode": 1
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/spy-brain.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#F2E7FF"
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "06851116-ebd0-4f34-a726-bed981c1862a"
      }
    }
  }
}
